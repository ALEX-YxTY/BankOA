<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.meishipintu.bankoa">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".views.activitys.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".views.activitys.NoticActivity" />
        <activity android:name=".views.activitys.TaskTriggerActivity" />
        <activity android:name=".views.activitys.ClerkListActivity" />
        <activity android:name=".views.activitys.LoginActivity" />
        <activity android:name=".views.activitys.RegisterActivity"></activity>
    </application>

</manifest>